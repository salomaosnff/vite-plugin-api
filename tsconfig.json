{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["es2022"],
        "downlevelIteration": true,
        "skipLibCheck": true,
        "moduleResolution": "Node",
        "module": "ES6",
        "types": [
            "openapi-types"
        ],
        "outDir": "dist",
        "declaration": true,
        "declarationDir": "dist"
    },
    "include": ["src"],
    "exclude": ["example", "dist", "node_modules/**/*"]
}